		// TODO Auto-generated method stub

		File f1= new File("../../abc.txt");
		
		System.out.println(f1.exists());
		
		try {
			boolean result=f1.createNewFile();
			System.out.println(result);
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		
		
		
		
		
		
		
		
	}

}
